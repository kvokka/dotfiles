{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  // https://github.com/code-yeongyu/oh-my-opencode/blob/master/docs/configurations.md
  "agents": {
    "sisyphus": {
      "model": "anthropic/gemini-claude-opus-4-5-thinking"
    },
    "librarian": {
      "model": "google/gemini-3-flash-preview"
    },
    "explore": {
      "model": "google/gemini-3-flash-preview"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash-preview"
    },
    "prometheus": {
      "model": "anthropic/gemini-claude-opus-4-5-thinking"
    },
    "metis": {
      "model": "anthropic/gemini-claude-sonnet-4-5"
    },
    "momus": {
      "model": "anthropic/gemini-claude-opus-4-5-thinking"
    },
    "atlas": {
      "model": "anthropic/gemini-claude-sonnet-4-5-thinking"
    }
  },
  "categories": {
    "artistry": {
      "model": "google/gemini-3-pro-preview"
    },
    "quick": {
      "model": "google/gemini-3-flash-preview"
    },
    "unspecified-low": {
      "model": "anthropic/gemini-claude-sonnet-4-5-thinking"
    },
    "unspecified-high": {
      "model": "anthropic/gemini-claude-opus-4-5-thinking"
    },
    "writing": {
      "model": "google/gemini-3-flash-preview"
    }
  },
  "experimental": {
    "aggressive_truncation": true,
    "auto_resume": true
  },
  "background_task": {
    "defaultConcurrency": 10,
    "providerConcurrency": {
      "google": 20,
      "anthropic": 1,
      "openai": 3
    },
    "modelConcurrency": {
      "google/gemini-3-flash-preview": 15,
      "google/gemini-3-flash": 15,
      "google/gemini-3-pro-preview": 10,
      "google/gemini-3-pro": 10,
      "anthropic/gemini-claude-opus-4-5-thinking": 1,
      "anthropic/gemini-claude-sonnet-4-5-thinking": 1,
      "openai/gpt-5.2-codex": 3,
      "openai/gpt-5.2": 3
    }
  }
}
