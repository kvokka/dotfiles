{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  // https://github.com/code-yeongyu/oh-my-opencode/blob/master/docs/configurations.md
  "agents": {
    "sisyphus": {
      "model": "anthropic/claude-opus-4-6-thinking"
    },
    "librarian": {
      "model": "google/gemini-3-flash-preview"
    },
    "explore": {
      "model": "google/gemini-3-flash-preview"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash-preview"
    },
    "prometheus": {
      "model": "anthropic/claude-opus-4-6-thinking"
    },
    "metis": {
      "model": "anthropic/claude-sonnet-4-6"
    },
    "momus": {
      "model": "anthropic/claude-opus-4-6-thinking"
    },
    "atlas": {
      // "model": "anthropic/gemini-claude-sonnet-4-5-thinking"
      "model": "opencode/kimi-k2.5-free"
    }
  },
  "categories": {
    "artistry": {
      "model": "google/gemini-3.1-pro-high"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "xhigh"
    },
    "quick": {
      "model": "google/gemini-3-flash-preview"
    },
    "unspecified-low": {
      "model": "anthropic/claude-sonnet-4-6"
    },
    "unspecified-high": {
      "model": "anthropic/claude-opus-4-6-thinking",
      "variant": "max"
    },
    "writing": {
      "model": "google/gemini-3-flash-preview"
    }
  },
  "experimental": {
    "aggressive_truncation": true,
    //   "auto_resume": true
  },
  "browser_automation_engine": {
    "provider": "agent-browser"
  },
  "background_task": {
    "defaultConcurrency": 10,
    "staleTimeoutMs": 180000,
    "providerConcurrency": {
      "google": 20,
      "anthropic": 3,
      "openai": 3
    },
    "modelConcurrency": {
      "google/gemini-3-flash-preview": 15,
      "google/gemini-3.1-pro-high": 10,
      "google/gemini-3.1-pro-low": 10,
      "anthropic/claude-opus-4-6-thinking": 3,
      "anthropic/claude-sonnet-4-6": 3,
      "anthropic/gemini-claude-sonnet-4-5-thinking": 3,
      "openai/gpt-5.3-codex": 3
    }
  }
}
