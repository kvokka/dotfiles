      "command": [
        "rumdl",
        "fmt",
        "--fix",
        "$FILE"
      ],
      "extensions": [
        ".md"
      ]
    },
    "ruff-check": {
      "command": [
        "ruff",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "ruff-format": {
      "command": [
        "ruff",
        "format",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "python-types-checker": {
      "command": [
        "ty",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "biome-check": {
      "command": [
        "biome",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        "json",
        "css"
      ]
    }
