{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode",
    // "opencode-websearch-cited"
  ],
  "server": {
    "hostname": "0.0.0.0",
    "mdns": true,
    "cors": [
      "http://localhost:5173"
    ]
  },
  "formatter": {
    "markdown-formatter": {
      "command": [
        "rumdl",
        "fmt",
        "--fix",
        "$FILE"
      ],
      "extensions": [
        ".md"
      ]
    },
    "ruff-check": {
      "command": [
        "ruff",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "ruff-format": {
      "command": [
        "ruff",
        "format",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "python-types-checker": {
      "command": [
        "ty",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "biome-check": {
      "command": [
        "biome",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        "json",
        "css"
      ]
    }
  },
