{
  "plugin": [
    "opencode-antigravity-auth@1.2.7",
    "oh-my-opencode",
    "opencode-openai-codex-auth"
  ],
  "server": {
    "port": 4096,
    "hostname": "0.0.0.0",
    "mdns": true,
    "cors": [
      "http://localhost:5173"
    ]
  },
  "formatter": {
    "markdown-formatter": {
      "command": [
        "rumdl",
        "fmt",
        "--fix",
        "$FILE"
      ],
      "extensions": [
        ".md"
      ]
    },
    "python-types-checker": {
      "command": [
        "ty",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    }
  },
  "model": "google/antigravity-gemini-3-pro-high",
  "small_model": "google/antigravity-gemini-3-flash",
  "provider": {
    "openai": {
      "name": "OpenAI",
      "api": "codex",
      "models": {
        "gpt-5.2": {
          "name": "GPT-5.2"
        },
        "gpt-5.2-codex-low": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "low"
          }
        },
        "gpt-5.2-codex": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "medium"
          }
        },
        "gpt-5.2-codex-high": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "high"
          }
        },
        "gpt-5.2-codex-xhigh": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "xhigh"
          }
        },
        "codex-1": {
          "name": "Codex-1"
        }
      }
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro-low": {
          "name": "Gemini 3 Pro Low (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
