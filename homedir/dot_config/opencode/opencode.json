{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@3.3.1",
    //  "opencode-websearch-cited"
  ],
  "server": {
    "hostname": "0.0.0.0",
    "mdns": true,
    "cors": [
      "http://localhost:5173"
    ]
  },
  "mcp": {
    "render": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "mcp-remote",
        "https://mcp.render.com/mcp",
        "--header",
        "Authorization: Bearer ${RENDER_API_KEY}"
      ],
      "enabled": false,
      "environment": {
        "Authorization": "Bearer ${RENDER_API_KEY}"
      },
    },
    "oh-my-posh-validator": {
      "type": "remote",
      "enabled": false,
      "url": "https://ohmyposh.dev/api/mcp"
    }
  },
  "formatter": {
    "markdown-formatter": {
      "command": [
        "rumdl",
        "fmt",
        "--fix",
        "$FILE"
      ],
      "extensions": [
        ".md"
      ]
    },
    "ruff-check": {
      "command": [
        "ruff",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "ruff-format": {
      "command": [
        "ruff",
        "format",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "python-types-checker": {
      "command": [
        "ty",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "biome-check": {
      "command": [
        "biome",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        "json",
        "css"
      ]
    }
  },
  "agent": {
    "title": {
      "model": "google/gemini-3-flash-preview"
    },
    "summary": {
      "model": "google/gemini-3-flash-preview"
    },
    "explore": {
      "model": "google/gemini-3-flash-preview"
    },
    "compaction": {
      "model": "google/gemini-3-pro-preview",
    },
    "general": {
      "model": "anthropic/gemini-claude-sonnet-4-5"
    }
  },
  "model": "google/gemini-3-pro-preview",
  "small_model": "google/gemini-3-flash-preview",
  "enabled_providers": [
    "opencode",
    "openai",
    "google",
    "anthropic"
  ],
  "provider": {
    // Providers are the reflections of the Quotio created for compatibility with other tools
    "anthropic": {
      "models": {
        "gemini-claude-opus-4-5-thinking": {
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "name": "Quotio Claude Opus 4 5 Thinking",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          },
          "options": {
            "thinking": {
              "budgetTokens": 10000,
              "type": "enabled"
            }
          },
          "reasoning": true
        },
        "gemini-claude-sonnet-4-5": {
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "name": "Quotio Claude Sonnet 4 5"
        },
        "gemini-claude-sonnet-4-5-thinking": {
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "name": "Quotio Claude Sonnet 4 5 Thinking",
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          },
          "options": {
            "thinking": {
              "budgetTokens": 10000,
              "type": "enabled"
            }
          },
          "reasoning": true
        },
      },
      "npm": "@ai-sdk/anthropic",
      "options": {
        "apiKey": "{env:QUOTIO_PROXY_API_KEY}",
        "baseURL": "{env:QUOTIO_PROXY_FULL_URL}"
      },
      "whitelist": [
        "gemini-claude-opus-4-5-thinking",
        "gemini-claude-sonnet-4-5",
        "gemini-claude-sonnet-4-5-thinking"
      ]
    },
    "openai": {
      "models": {
        "gpt-5.2": {
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "name": "Quotio Gpt 5.2",
          "options": {
            "reasoningEffort": "high",
            "textVerbosity": "medium"
          },
          "variants": {
            "low": {
              "reasoningEffort": "low",
              "textVerbosity": "medium"
            },
            "high": {
              "reasoningEffort": "high",
              "textVerbosity": "medium"
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "textVerbosity": "medium"
            }
          },
          "reasoning": true
        },
        "gpt-5.3-codex": {
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "name": "Quotio Gpt 5.3 Codex",
          "options": {
            "reasoningEffort": "high",
            "textVerbosity": "medium"
          },
          "variants": {
            "low": {
              "reasoningEffort": "low",
              "textVerbosity": "medium"
            },
            "high": {
              "reasoningEffort": "high",
              "textVerbosity": "medium"
            },
            "xhigh": {
              "reasoningEffort": "xhigh",
              "textVerbosity": "medium"
            }
          },
          "reasoning": true
        },
        "gpt-5.2-codex-mini": {
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "name": "Quotio Gpt 5.2 Codex Mini",
          "options": {
            "reasoning": {
              "effort": "low"
            }
          },
          "reasoning": true
        },
        "gpt-oss-120b-medium": {
          "limit": {
            "context": 400000,
            "output": 32768
          },
          "name": "Quotio Gpt Oss 120B Medium"
        }
      },
      "name": "OpenAI",
      "npm": "@ai-sdk/openai",
      "options": {
        "apiKey": "{env:QUOTIO_PROXY_API_KEY}",
        "baseURL": "{env:QUOTIO_PROXY_FULL_URL}"
      },
      "whitelist": [
        "gpt-5.2",
        "gpt-5.3-codex",
        "gpt-5.2-codex-mini",
        "gpt-oss-120b-medium"
      ]
    },
    "google": {
      "models": {
        "gemini-2.5-flash": {
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "name": "Quotio Gemini 2.5 Flash"
        },
        "gemini-2.5-flash-lite": {
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "name": "Quotio Gemini 2.5 Flash Lite"
        },
        "gemini-3-flash-preview": {
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "high": {
              "thinkingLevel": "high"
            },
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            }
          },
          "name": "Quotio Gemini 3 Flash Preview"
        },
        "gemini-3-pro-image-preview": {
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "high": {
              "thinkingLevel": "high"
            },
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            }
          },
          "name": "Quotio Gemini 3 Pro Image Preview"
        },
        "gemini-3-pro-preview": {
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "high": {
              "thinkingLevel": "high"
            },
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            }
          },
          "name": "Quotio Gemini 3 Pro Preview"
        }
      },
      "whitelist": [
        "gemini-2.5-flash",
        "gemini-2.5-flash-lite",
        "gemini-3-flash-preview",
        "gemini-3-pro-image-preview",
        "gemini-3-pro-preview"
      ],
      "name": "Google",
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "websearch_cited": {
          "model": "gemini-3-flash-preview"
        },
        "apiKey": "{env:QUOTIO_PROXY_API_KEY}",
        "baseURL": "{env:QUOTIO_PROXY_FULL_URL}"
      },
    }
  }
}
