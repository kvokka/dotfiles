{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "opencode-antigravity-auth@1.2.8",
    "oh-my-opencode",
    "opencode-openai-codex-auth",
    "opencode-websearch-cited"
  ],
  "server": {
    "port": 4096,
    "hostname": "0.0.0.0",
    "mdns": true,
    "cors": [
      "http://localhost:5173"
    ]
  },
  "formatter": {
    "markdown-formatter": {
      "command": [
        "rumdl",
        "fmt",
        "--fix",
        "$FILE"
      ],
      "extensions": [
        ".md"
      ]
    },
    "ruff-check": {
      "command": [
        "ruff",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "ruff-format": {
      "command": [
        "ruff",
        "format",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "python-types-checker": {
      "command": [
        "ty",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".py"
      ]
    },
    "biome-check": {
      "command": [
        "biome",
        "check",
        "$FILE"
      ],
      "extensions": [
        ".ts",
        ".tsx",
        "json",
        "css"
      ]
    }
  },
  "model": "google/antigravity-gemini-3-pro",
  "small_model": "google/antigravity-gemini-3-flash",
  "provider": {
    "openai": {
      "name": "OpenAI",
      "api": "codex",
      "models": {
        "gpt-5.2": {
          "name": "GPT-5.2"
        },
        "gpt-5.2-codex-low": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "low"
          }
        },
        "gpt-5.2-codex": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "medium"
          }
        },
        "gpt-5.2-codex-high": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "high"
          }
        },
        "gpt-5.2-codex-xhigh": {
          "name": "GPT-5.2-codex",
          "options": {
            "reasoningEffort": "xhigh"
          }
        },
        "codex-1": {
          "name": "Codex-1"
        }
      },
      "options": {
        "websearch_cited": {
          "model": "gpt-5.2"
        }
      }
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (no thinking) (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        }
      },
      "options": {
        "websearch_cited": {
          "model": "gemini-3-flash-preview"
        }
      }
    }
  }
}
